.index_swiper swiper{
  width:750rpx;
  height:340rpx;
}
.index_swiper image{
  width:100%;
}
/* 导航 */
.index_cata{
  display:flex;
}
.index_cata navigator{
  padding: 20rpx;
  flex:1;
}
.index_cata image{
  width:100%;
}
/* 楼层开始 */
.index_floor{

}
.floor_group{}
.floor_title{
  padding: 10rpx 0; 
}
.floor_title image{
  width:100%;
}
.floor_list{
  overflow: hidden;
}
.floor_list navigator{
  float:left;
  width:33.3%;
}
.floor_list navigator:nth-last-child(-n+4){
  height: 27.72711207vw;
  border-left:10rpx solid #fff;
}
.floor_list navigator:nth-child(2),
.floor_list navigator:nth-child(3){
  border-bottom: 10rpx solid #fff;
}
.floor_list image{
  width:100%;
  height:100%;
}